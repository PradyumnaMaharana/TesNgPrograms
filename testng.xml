<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<parameter name="browser" value="chrome" />
	<parameter name="url"
		value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
	<test thread-count="5" name="Test">
		<groups>
			<run>
				<include name="Sanity" />
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="testNgPractice.AnnotationsPractice" />
			<class name="testNgPractice.PrioritizingAndDisablingPractice" />
			<class name="testNgPractice.DependencyTestPractice" />
			<class name="testNgPractice.GroupingPractice" />
			<class name="testNgPractice.ParameterizationPractice" />
			<class name="testNgPractice.DataProviderPractice" />
			<class name="testNgPractice.ListenerPractice" />
			<class name="automationUsingTestNG.CheckLogoIsDisplayed" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
